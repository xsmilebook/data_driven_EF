  "evaluation": {
    "cv": {
      "enabled": true,
      "n_splits": 5,
      "shuffle": true,
      "stratify": false,
      "random_state": 42
    },
    "permutation_test": {
      "enabled": true,
      "n_permutations": 1000,
      "random_state": 42
    },
    "nested_cv": {
      "enabled": false,
      "outer_splits": 5,
      "inner_splits": 3
    }
  },
  
  "output": {
    "results_dir": "/ibmgpfs/cuizaixu_lab/xuhaoshu/code/data_driven_EF/data/EFNY/results",
    "create_timestamp_subdirs": true,
    "save_formats": ["json", "npz"],
    "compression": {
      "enabled": true,
      "level": 6
    },
    "file_naming": {
      "prefix": "efny",
      "include_timestamp": true,
      "include_model_type": true,
      "include_task_id": true
    }
  },
  
  "logging": {
    "level": "INFO",
    "format": "%(asctime)s - %(name)s - %(levelname)s - %(message)s",
    "file_logging": {
      "enabled": true,
      "log_dir": "data/EFNY/results/logs",
      "max_file_size_mb": 100,
      "backup_count": 5
    },
    "console_logging": {
      "enabled": true,
      "use_colors": true
    }
  },
  
  "hpc": {
    "parallel": {
      "enabled": true,
      "n_jobs": -1,
      "backend": "loky"
    },
    "memory_efficient": {
      "enabled": true,
      "batch_size": 50
    },
    "checkpointing": {
      "enabled": true,
      "interval": 100,
      "dir": "data/EFNY/results/checkpoints"
    }
  },
  
  "validation": {
    "data_integrity_checks": {
      "enabled": true,
      "check_finite": true,
      "check_shapes": true,
      "check_missing_values": true
    },
    "model_validation": {
      "enabled": true,
      "check_convergence": true,
      "max_iterations_warning": 1000
    }
  }
