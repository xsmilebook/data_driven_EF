{
  "data": {
    "root_dir": "/ibmgpfs/cuizaixu_lab/xuhaoshu/code/data_driven_EF/data/EFNY",
    "brain_file": "fc_vector/Schaefer400/EFNY_Schaefer400_FC_matrix.npy",
    "behavioral_file": "table/metrics/EFNY_beh_metrics.csv",
    "covariates_file": "table/demo/EFNY_behavioral_data.csv",
    "sublist_file": "table/sublist/sublist.txt",
    "max_missing_rate": 0.1,
    "quality_check_enabled": true
  },
  
  "model": {
    "default_n_components": 5,
    "supported_models": ["pls", "scca"],
    "default_model": "pls",
    "pls": {
      "scale": true,
      "max_iter": 500,
      "tol": 1e-06
    },
    "scca": {
      "sparsity_X": 0.1,
      "sparsity_Y": 0.1,
      "max_iter": 1000,
      "tol": 1e-06
    }
  },
  
  "preprocessing": {
    "regress_confounds": true,
    "standardize_confounds": true,
    "confound_variables": ["sex", "age", "meanFD"],
    "quality_filtering": {
      "enabled": true,
      "max_missing_rate": 0.1,
      "min_std": 1e-06
    }
  },
  "behavioral": {
